{
  "manifest_version": 3,
  "name": "Extension Translate Subtitles",
  "description": "This extension translate subtitles from videos",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://rezka.ag/*"
      ]
    }
  ]
}

